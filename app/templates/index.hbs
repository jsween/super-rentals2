<h1>Welcome to Super Rentals</h1>

<h3>Available Rentals</h3>

<ul>
  {{#each model.rentals as |rental|}}
    {{rental-tile update="update" rental=rental destroyRental="destroyRental"}}
  {{/each}}
</ul>

{{new-rental save="save"}}

<h3>Cities</h3>
<ul>
  {{#each model.cities as |city|}}
    <li>{{city-tile city=city}}</li>
  {{/each}}
</ul>
